#include<stdio.h>
main()
{
	char ch;
	printf("enter the alphabet to be checked = ");
	scanf("%c",&ch);
	if(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u'||ch=='A'||ch=='E'||ch=='I'||ch=='O'||ch=='U')
	{
		printf("\n the alphabet is vowel");
	}
	else
	{
		printf("\n the alphabet is a consonant");
	}
	
}
